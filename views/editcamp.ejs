<% layout('./boilerplate/main') %>
    <form method="post"action="/campgrounds/<%= campground._id %>?_method=put">
        <label for="title">title</label>
        <input type="text" name="title" id="title" value="<%= campground.title%>">
        <label for="location">location</label>
        <input type="text" name="location" id="location" value="<%= campground.location %>">
        <button>Submit</button>
    </form>
